{
  "name": "pmdmml-syntax",
  "displayName": "PMDMML-Syntax",
  "description": "Provides syntax highlighting for M.Kajihara's Professional Music Driver MML",
  "version": "1.0.7",
  "publisher": "Juubmnh",
  "repository": "https://github.com/Juubmnh/pmdmml-syntax",
  "license": "MIT",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "pmdmml",
        "aliases": [
          "PMDMML",
          "pmdmml"
        ],
        "extensions": [
          ".mml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "configuration": [
      {
        "title": "PMDMML Config",
        "properties": {
          "pmdmml-syntax.batchPath": {
            "type": "string",
            "description": "Used for compiling .MML files"
          },
          "pmdmml-syntax.pattern1": {
            "type": "string",
            "default": ";.*|`[^`]*`"
          },
          "pmdmml-syntax.pattern2": {
            "type": "string",
            "default": "^#.*|~[+-]?\\d+|[\"'/]|\\|!?[A-Za-z]*"
          },
          "pmdmml-syntax.pattern3": {
            "type": "string",
            "default": "^@(?:\\s*\\d+){3}(?:\\s*=.*)?$|(?<!^\\S*)@+\\d+(?:,\\d+){0,3}|(?<!\\|)!(?:\\d+|\\S+)"
          },
          "pmdmml-syntax.pattern4": {
            "type": "string",
            "default": "R\\d+|^[A-Za-z]+"
          },
          "pmdmml-syntax.pattern5": {
            "type": "string",
            "default": "[\\[:L]|\\]\\d*"
          },
          "pmdmml-syntax.pattern6": {
            "type": "string",
            "default": "(?<!^\\S*|\\\\\\S*)o[+-]?\\d+|X|l%?\\d+\\.*|[l ](?:[=+-]?\\d+\\.*|\\^\\d+)|C\\d+|Q%?\\d+|q(?:\\d*|l\\d+\\.*)?(?:-(?:\\d*|l\\d+)?)?(?:,(?:\\d+|l\\d+\\.*))?|__?[+-]\\d+|_{[+\\-=][cdefgab\\s]*}|_M[+-]\\d+|v[+\\-)(]?\\d+|V\\d+|[)(]\\^?%?\\d*|s\\d+|O\\d+,[+-]?\\d+|FB[+-]?\\d+|P\\d+|w[+-]?\\d+|DD?[+-]?\\d+|sdd?\\d+,[+-]?\\d+|DX\\d+|B\\d+|I[+-]?\\d+|DM[+-]?\\d+|E\\d+(?:,[+-]?\\d+(?:,\\d+){2}|(?:,\\d+){4,5})|EX\\d+|M[AB]?(?:\\d+|l\\d+\\.*)(?:(?:,\\d+)(?:,[+-]?\\d+)(?:,\\d+))?|MW[AB]?\\d+|\\*\\d+(?:,\\d+)?|\\*A\\d+(?:,B?\\d+)?|\\*B\\d+(?:,A\\d+)?|MM[AB]?\\d+|MX[AB]?\\d+|MP[AB]?[+-]\\d+(?:,(?:\\d+|l\\d+\\.*)(?:,\\d+)?)?|MD[AB]?\\d+(?:,[+-]\\d+(?:,\\d+)?)?|H\\d+(?:,\\d+)?(?:,(?:\\d+|l\\d+\\.*))?|#\\d+(?:,\\d+)?|##\\d+,\\d+,[+-]\\d+,\\d+|#[fwa]\\d+|#p[+-]\\d+|#D(?:\\d+|l\\d+\\.*)|t[+-]?\\d+|T[+-]?\\d+|S(?:\\d+|l\\d+\\.*)(?:,[+-]\\d+(?:,\\d+)?)?|W(?:\\d+|l\\d+\\.*)(?:,%?[+-]\\d+(?:,\\d+)?)?|sk\\d+(?:,(?:\\d+|l\\d+\\.*))?|{{(?:(?:o\\d+|[<>\\s])*[cdefgab]){1,16}(?:o\\d+|[<>\\s])*}}\\d*\\.*(?:,\\d+(?:,\\d+(?:,\\d+(?:,[+-]\\d+)?)?)?)?|p\\d+|px[+-]\\d+(?:,\\d+)?|\\\\[bschti]p?|\\\\V[+-]?\\d+|\\\\v[bschti][+-]?\\d+|\\\\[lmr][bschti]|y\\d+,\\d+|\\*\\d+|D\\d+|D[FSPR][+-]?\\d+|N\\d+|n\\d+|m\\d+|Z\\d+|A\\d+"
          },
          "pmdmml-syntax.style1": {
            "type": "string",
            "default": "#727169|italic"
          },
          "pmdmml-syntax.style2": {
            "type": "string",
            "default": "#E6C384|italic"
          },
          "pmdmml-syntax.style3": {
            "type": "string",
            "default": "#D27E99"
          },
          "pmdmml-syntax.style4": {
            "type": "string",
            "default": "#957FB8||800"
          },
          "pmdmml-syntax.style5": {
            "type": "string",
            "default": "#9CABCA"
          },
          "pmdmml-syntax.style6": {
            "type": "string",
            "default": "#76946A"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "pmdmml-syntax.compile",
        "title": "Compile To .M"
      },
      {
        "command": "pmdmml-syntax.convertNum",
        "title": "Convert Number"
      },
      {
        "command": "pmdmml-syntax.runTool",
        "title": "Run Timbre Trial"
      }
    ],
    "keybindings": [
      {
        "command": "pmdmml-syntax.compile",
        "win": "ctrl+s",
        "linux": "ctrl+s",
        "mac": "cmd+s",
        "when": "editorTextFocus && resourceExtname =~ /\\.mml/i"
      },
      {
        "command": "pmdmml-syntax.convertNum",
        "win": "f2",
        "linux": "f2",
        "mac": "f2",
        "when": "editorTextFocus && resourceExtname =~ /\\.mml/i"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "pmdmml-syntax.compile",
          "group": "navigation",
          "when": "editorTextFocus && resourceExtname =~ /\\.mml/i"
        },
        {
          "command": "pmdmml-syntax.runTool",
          "group": "navigation",
          "when": "editorTextFocus && resourceExtname =~ /\\.mml/i && isWindows"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}