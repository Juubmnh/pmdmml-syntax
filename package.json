{
  "name": "pmdmml-syntax",
  "displayName": "PMDMML-Syntax",
  "description": "Provides syntax highlighting for M.Kajihara's Professional Music Driver MML",
  "version": "1.0.6",
  "publisher": "Juubmnh",
  "repository": "https://github.com/Juubmnh/pmdmml-syntax",
  "license": "MIT",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "pmdmml",
        "aliases": [
          "PMDMML",
          "pmdmml"
        ],
        "extensions": [
          ".mml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "configuration": [
      {
        "title": "PMDMML Config",
        "properties": {
          "pmdmml-syntax.batchPath": {
            "type": "string",
            "description": "Used for compiling .MML files"
          },
          "pmdmml-syntax.pattern1": {
            "type": "string",
            "default": ";.*|`[^`]*`"
          },
          "pmdmml-syntax.pattern2": {
            "type": "string",
            "default": "^#.*|~[+-]?(?:\\d|\\$[0-9A-Fa-f])+|[\"'/]|\\|!?[A-Za-z]*"
          },
          "pmdmml-syntax.pattern3": {
            "type": "string",
            "default": "^@(?:\\s*(?:\\d|\\$[0-9A-Fa-f])+){3}(?:\\s*=.*)?$|(?<!^\\S*)@+(?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+){0,3}|(?<!\\|)!\\S+"
          },
          "pmdmml-syntax.pattern4": {
            "type": "string",
            "default": "R(?:\\d|\\$[0-9A-Fa-f])+|^[A-Za-z]+"
          },
          "pmdmml-syntax.pattern5": {
            "type": "string",
            "default": "[\\[:L]|\\]\\d*"
          },
          "pmdmml-syntax.pattern6": {
            "type": "string",
            "default": "(?<!^\\S*|\\\\\\S*)o[+-]?(?:\\d|\\$[0-9A-Fa-f])+|X|l%?(?:\\d|\\$[0-9A-Fa-f])+\\.*|[l ](?:[=+-]?(?:\\d|\\$[0-9A-Fa-f])+\\.*|\\^(?:\\d|\\$[0-9A-Fa-f])+)|C(?:\\d|\\$[0-9A-Fa-f])+|Q%?(?:\\d|\\$[0-9A-Fa-f])+|q(?:(?:\\d|\\$[0-9A-Fa-f])*|l(?:\\d|\\$[0-9A-Fa-f])+\\.*)?(?:-(?:(?:\\d|\\$[0-9A-Fa-f])*|l(?:\\d|\\$[0-9A-Fa-f])+)?)?(?:,(?:(?:\\d|\\$[0-9A-Fa-f])+|l(?:\\d|\\$[0-9A-Fa-f])+\\.*))?|__?[+-](?:\\d|\\$[0-9A-Fa-f])+|_{[+\\-=][cdefgab\\s]*}|_M[+-](?:\\d|\\$[0-9A-Fa-f])+|v[+\\-)(]?(?:\\d|\\$[0-9A-Fa-f])+|V(?:\\d|\\$[0-9A-Fa-f])+|[)(]\\^?%?(?:\\d|\\$[0-9A-Fa-f])*|s(?:\\d|\\$[0-9A-Fa-f])+|O(?:\\d|\\$[0-9A-Fa-f])+,[+-]?(?:\\d|\\$[0-9A-Fa-f])+|FB[+-]?(?:\\d|\\$[0-9A-Fa-f])+|P(?:\\d|\\$[0-9A-Fa-f])+|w[+-]?(?:\\d|\\$[0-9A-Fa-f])+|DD?[+-]?(?:\\d|\\$[0-9A-Fa-f])+|sdd?(?:\\d|\\$[0-9A-Fa-f])+,[+-]?(?:\\d|\\$[0-9A-Fa-f])+|DX(?:\\d|\\$[0-9A-Fa-f])+|B(?:\\d|\\$[0-9A-Fa-f])+|I[+-]?(?:\\d|\\$[0-9A-Fa-f])+|DM[+-]?(?:\\d|\\$[0-9A-Fa-f])+|E(?:\\d|\\$[0-9A-Fa-f])+(?:,[+-]?(?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+){2}|(?:,(?:\\d|\\$[0-9A-Fa-f])+){4,5})|EX(?:\\d|\\$[0-9A-Fa-f])+|M[AB]?(?:(?:\\d|\\$[0-9A-Fa-f])+|l(?:\\d|\\$[0-9A-Fa-f])+\\.*)(?:(?:,(?:\\d|\\$[0-9A-Fa-f])+)(?:,[+-]?(?:\\d|\\$[0-9A-Fa-f])+)(?:,(?:\\d|\\$[0-9A-Fa-f])+))?|MW[AB]?(?:\\d|\\$[0-9A-Fa-f])+|\\*(?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+)?|\\*A(?:\\d|\\$[0-9A-Fa-f])+(?:,B?(?:\\d|\\$[0-9A-Fa-f])+)?|\\*B(?:\\d|\\$[0-9A-Fa-f])+(?:,A(?:\\d|\\$[0-9A-Fa-f])+)?|MM[AB]?(?:\\d|\\$[0-9A-Fa-f])+|MX[AB]?(?:\\d|\\$[0-9A-Fa-f])+|MP[AB]?[+-](?:\\d|\\$[0-9A-Fa-f])+(?:,(?:(?:\\d|\\$[0-9A-Fa-f])+|l(?:\\d|\\$[0-9A-Fa-f])+\\.*)(?:,(?:\\d|\\$[0-9A-Fa-f])+)?)?|MD[AB]?(?:\\d|\\$[0-9A-Fa-f])+(?:,[+-](?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+)?)?|H(?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+)?(?:,(?:(?:\\d|\\$[0-9A-Fa-f])+|l(?:\\d|\\$[0-9A-Fa-f])+\\.*))?|#(?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+)?|##(?:\\d|\\$[0-9A-Fa-f])+,(?:\\d|\\$[0-9A-Fa-f])+,[+-](?:\\d|\\$[0-9A-Fa-f])+,(?:\\d|\\$[0-9A-Fa-f])+|#[fwa](?:\\d|\\$[0-9A-Fa-f])+|#p[+-](?:\\d|\\$[0-9A-Fa-f])+|#D(?:(?:\\d|\\$[0-9A-Fa-f])+|l(?:\\d|\\$[0-9A-Fa-f])+\\.*)|t[+-]?(?:\\d|\\$[0-9A-Fa-f])+|T[+-]?(?:\\d|\\$[0-9A-Fa-f])+|S(?:(?:\\d|\\$[0-9A-Fa-f])+|l(?:\\d|\\$[0-9A-Fa-f])+\\.*)(?:,[+-](?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+)?)?|W(?:(?:\\d|\\$[0-9A-Fa-f])+|l(?:\\d|\\$[0-9A-Fa-f])+\\.*)(?:,%?[+-](?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+)?)?|sk(?:\\d|\\$[0-9A-Fa-f])+(?:,(?:(?:\\d|\\$[0-9A-Fa-f])+|l(?:\\d|\\$[0-9A-Fa-f])+\\.*))?|{{(?:(?:o(?:\\d|\\$[0-9A-Fa-f])+|[<>\\s])*[cdefgab]){1,16}(?:o(?:\\d|\\$[0-9A-Fa-f])+|[<>\\s])*}}(?:\\d|\\$[0-9A-Fa-f])*\\.*(?:,(?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+(?:,[+-](?:\\d|\\$[0-9A-Fa-f])+)?)?)?)?|p(?:\\d|\\$[0-9A-Fa-f])+|px[+-](?:\\d|\\$[0-9A-Fa-f])+(?:,(?:\\d|\\$[0-9A-Fa-f])+)?|\\\\[bschti]p?|\\\\V[+-]?(?:\\d|\\$[0-9A-Fa-f])+|\\\\v[bschti][+-]?(?:\\d|\\$[0-9A-Fa-f])+|\\\\[lmr][bschti]|y(?:\\d|\\$[0-9A-Fa-f])+,(?:\\d|\\$[0-9A-Fa-f])+|\\*(?:\\d|\\$[0-9A-Fa-f])+|D(?:\\d|\\$[0-9A-Fa-f])+|D[FSPR][+-]?(?:\\d|\\$[0-9A-Fa-f])+|N(?:\\d|\\$[0-9A-Fa-f])+|n(?:\\d|\\$[0-9A-Fa-f])+|m(?:\\d|\\$[0-9A-Fa-f])+|Z(?:\\d|\\$[0-9A-Fa-f])+|A(?:\\d|\\$[0-9A-Fa-f])+"
          },
          "pmdmml-syntax.style1": {
            "type": "string",
            "default": "#727169|italic"
          },
          "pmdmml-syntax.style2": {
            "type": "string",
            "default": "#E6C384|italic"
          },
          "pmdmml-syntax.style3": {
            "type": "string",
            "default": "#D27E99"
          },
          "pmdmml-syntax.style4": {
            "type": "string",
            "default": "#957FB8||800"
          },
          "pmdmml-syntax.style5": {
            "type": "string",
            "default": "#9CABCA"
          },
          "pmdmml-syntax.style6": {
            "type": "string",
            "default": "#76946A"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "pmdmml-syntax.compile",
        "title": "Compile to .M"
      },
      {
        "command": "pmdmml-syntax.runTool",
        "title": "Run timbre trial"
      }
    ],
    "keybindings": [
      {
        "command": "pmdmml-syntax.compile",
        "win": "ctrl+s",
        "linux": "ctrl+s",
        "mac": "cmd+s",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "pmdmml-syntax.compile",
          "group": "navigation",
          "when": "resourceExtname =~ /\\.mml/i"
        },
        {
          "command": "pmdmml-syntax.runTool",
          "group": "navigation",
          "when": "resourceExtname =~ /\\.mml/i && isWindows"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}